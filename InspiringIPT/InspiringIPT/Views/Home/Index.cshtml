@model IEnumerable<InspiringIPT.Models.Cursos>

@{
    ViewBag.Title = "Inspiring IPTomar";
}


<!-- Header Carousel -->
<header id="entrada" class="carousel slide">
    @if (!User.Identity.IsAuthenticated) // TempData["LogSuccess"] != null
    {
        <div id="myModal" class="modal fade" role="dialog">
            <div class="modal-dialog">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header modal-header-success">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">@TempData["is"]</h4>
                    </div>
                    <div class="modal-body">
                        
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-success" data-dismiss="modal">Fechar</button>
                    </div>
                </div>
            </div>
        </div>
    }

    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#entrada" data-slide-to="0" class="active"></li>
        <li data-target="#entrada" data-slide-to="1"></li>
        <li data-target="#entrada" data-slide-to="2"></li>
        <li data-target="#entrada" data-slide-to="3"></li>
    </ol>
    <!-- Wrapper for slides -->
    <br />
    <div class="carousel-inner" role="listbox"/>
        <div class="item active">
            <img src="~/Imags/banner/banner_4.png" alt="Chania" />
            <div class="carousel-caption">
            </div>
        </div>

    <div class="item">
        <img src="~/Imags/banner/banner_7.png" alt="Chania" />
        <div class="carousel-caption">
        </div>
    </div>

        <div class="item">
